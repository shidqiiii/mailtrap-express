{
  "name": "mailtrap-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "set NODE_ENV=development&& nodemon ./bin/www",
    "migrate:dev": "set NODE_ENV=development&& npx sequelize-cli db:migrate",
    "seed:dev": "set NODE_ENV=development&& npx sequelize-cli db:seed:all",
    "reset:dev": "set NODE_ENV=development&& npx sequelize-cli db:drop&& npx sequelize-cli db:create&& npx sequelize-cli db:migrate"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "email-templates": "^11.1.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "morgan": "~1.9.1",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.2",
    "sequelize": "^6.32.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1"
  }
}
